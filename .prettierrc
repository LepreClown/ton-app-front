{
  "tabWidth": 2,
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": true,
  "printWidth": 100,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/screens/(.*)$",
    "^@/components/(.*)$",
    "^@/ui/(.*)$",
    "^@/hooks/(.*)$",
    "^@/services/(.*)$",
    "^@/providers]/(.*)$",
    "^@/assets/(.*)$",
    "^@/utils/(.*)$",
    "^@/build/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$",
    "(.sass)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}